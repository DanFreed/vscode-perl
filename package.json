{
  "name": "vscode-perl",
  "displayName": "Perl",
  "description": "Perl completions and more via ctags.",
  "version": "0.4.0",
  "author": "Henrik Sjööh <hello@enhenrik.nu> (http://www.enhenrik.nu)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/henriiik/vscode-perl"
  },
  "bugs": {
    "url": "https://github.com/henriiik/vscode-perl/issues"
  },
  "homepage": "https://github.com/henriiik/vscode-perl",
  "categories": [
    "Languages"
  ],
  "publisher": "henriiik",
  "engines": {
    "vscode": "^1.10.0"
  },
  "activationEvents": [
    "onLanguage:perl"
  ],
  "contributes": {
    "commands": [
      {
        "command": "perl.generateTags",
        "title": "Perl: Generate Tags"
      }
    ]
  },
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.52",
    "mocha": "^2.3.3",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "vscode": "^1.0.5"
  }
}